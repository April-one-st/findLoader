/* pages/authentication/authentication.wxss */
.authentication-page{
  display: flex;
  flex-direction: column;
  height: 100vh;
  background-color: rgba(245, 247, 250, 1);
  font-family: PingFang SC, PingFang SC;
}
.form-title{
  height: 46px;
  line-height: 46px;
  padding: 0 15px;
  font-size: 14px;
  color: rgba(102, 102, 102, 1);
  display: flex;
  justify-content: space-between;
}
.license-upLoad{
  background-color: #fff;
  padding: 10px 15px;
}
.license-upLoad-title{
  color: rgba(51, 51, 51, 1);
  font-size: 15px;
  line-height: 21px;
}
.license-upLoad-tips{
  color: rgba(153, 153, 153, 1);
  font-size: 12px;
  line-height: 17px;
  margin-bottom: 16px;
}
.license-upLoad-example{
  display: flex;
}
.example{
  width: 115px;
  height: 115px;
  margin-right: 10px;
}
.file-upLoad{
  width: 115px;
  height: 115px;
}
.van-uploader__upload{
  width: 115px !important;
  height: 115px !important;
}
.bottom{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 54px;
  padding-bottom: 34px;
  background-color: #fff;
}
.form-buttom{
  margin-top: 18px;
  display: flex;
  justify-content: center;
  align-items: center;
}