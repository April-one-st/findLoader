/* pages/components/footer/footer.wxss */
.footer {
  position: fixed;
  bottom: 0px;
  padding-bottom: 20px;
  z-index: 9;
  width: 100%;
  border-top: 1px solid #ccc;
  flex: 1;
  background-color: #fff;
  font-family: PingFang SC, PingFang SC;
}
.footer-conter {
  display: flex;
  justify-content: space-between;
}
.list-item {
  width: 94px;
  height: 50px;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 11px;
  color: #A7A8AC;
  margin-top: 7px;
}
.icon{
  width: 28px;
  height: 28px;
  margin-bottom: 3px;
}